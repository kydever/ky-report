    "element-plus": "^2.1.4",
    "js-cookie": "^3.0.1",
    "path-to-regexp": "^6.2.0",
    "pinia": "^2.0.12",
    "rollup-plugin-esbuild": "^4.8.2",
    "sass": "^1.49.9",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "stylelint": "^14.6.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.6.6",
    "unplugin-vue-components": "^0.18.0",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint --fix"
